{
    "license": "MIT",
    "name": "@ubiquity/ui",
    "prettier": {
        "arrowParens": "always",
        "bracketSpacing": true,
        "cursorOffset": -1,
        "embeddedLanguageFormatting": "auto",
        "endOfLine": "lf",
        "htmlWhitespaceSensitivity": "css",
        "insertPragma": false,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "pluginSearchDirs": [],
        "plugins": [],
        "printWidth": 80,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "rangeStart": 0,
        "requirePragma": false,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 2,
        "trailingComma": "es5",
        "useTabs": true,
        "vueIndentScriptAndStyle": false
    },
    "version": "0.0.0",
    "private": true,
    "dependencies": {
        "@types/sha256": "^0.2.0",
        "sha256": "^0.2.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/jest": "^26.0.16",
        "@types/node": "^14.14.10",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/web3": "^1.2.2",
        "bootstrap": "^4.4.1",
        "change-case": "^4.1.2",
        "ethers": "^5.1.3",
        "react": "^17.0.1",
        "react-dom": "^16.13.1",
        "react-draggable": "^4.4.3",
        "react-scripts": "^4.0.3",
        "title-case": "^3.0.3",
        "typescript": "^4.1.2",
        "web3": "^1.3.0"
    },
    "scripts": {
        "prestart": "yarn react-scripts start",
        "start": "open localhost:8888",
        "build": "yarn react-scripts build",
        "test": "yarn react-scripts test",
        "eject": "yarn react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}